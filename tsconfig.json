{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@database/*": [
        "./src/database/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
    }
  }
}
